package com.vyom.validator;

import java.io.Serializable;

public interface EnumType extends Serializable {
	
	/**
	 * This method returns id of enum
	 * @return id of enum
	 */
    public int getId();
    
    /**
	 * This method returns name of enum
	 * @return name of enum
	 */
    public String getName();
}