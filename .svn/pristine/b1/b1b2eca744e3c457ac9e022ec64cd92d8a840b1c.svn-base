package com.vyom.chatbot.core;

public class DataCollectionDetails {

	String displayText;
	String inputName;
	String userInput;
	String validationMethod;
	
	public DataCollectionDetails(String displayText, String inputName, String userInput, String validationMethod) {
		super();
		this.displayText = displayText;
		this.inputName = inputName;
		this.userInput = userInput;
		this.validationMethod = validationMethod;
	}
	public String getDisplayText() {
		return displayText;
	}
	public void setDisplayText(String displayText) {
		this.displayText = displayText;
	}
	public String getInputName() {
		return inputName;
	}
	public void setInputName(String inputName) {
		this.inputName = inputName;
	}
	public String getUserInput() {
		return userInput;
	}
	public void setUserInput(String userInput) {
		this.userInput = userInput;
	}
	public String getValidationMethod() {
		return validationMethod;
	}
	public void setValidationMethod(String validationMethod) {
		this.validationMethod = validationMethod;
	}

	@Override
	public String toString() {
		return "DataCollectionDetails [displayText=" + displayText + ", inputName=" + inputName + ", userInput="
				+ userInput + ", validationMethod=" + validationMethod + "]";
	}
	
}
